* Measure JNW_VIS_TI
.control

load {cicname}.raw

echo "MEAS_START"

#ifdef Nosweep
foreach vtemp 27
#else
foreach vtemp {temp_sweep}
#endif

    set fend = .raw
    load {cicname}_$vtemp$fend

    meas tran i_temp$vtemp find i(VSENS) at=45n
    meas tran vref$vtemp find v(VREF) at=45n

end

echo "MEAS_END"
.endc
